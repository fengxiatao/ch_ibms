      "productKey": "bullet_camera_001",
      "productName": "枪型摄像机",
      "category": "视频监控",
      "properties": [
        {
          "identifier": "online_status",
          "name": "在线状态",
          "description": "设备是否在线",
          "accessMode": "r",
          "required": true,
          "dataType": "bool",
          "dataSpecsList": [
            {"dataType": "bool", "name": "离线", "value": 0},
            {"dataType": "bool", "name": "在线", "value": 1}
          ]
        },
        {
          "identifier": "device_status",
          "name": "设备状态",
          "description": "设备运行状态",
          "accessMode": "r",
          "required": true,
          "dataType": "enum",
          "dataSpecsList": [
            {"dataType": "enum", "name": "离线", "value": 0},
            {"dataType": "enum", "name": "在线正常", "value": 1},
            {"dataType": "enum", "name": "在线异常", "value": 2}
          ]
        },
        {
          "identifier": "rtsp_url",
          "name": "RTSP地址",
          "description": "主码流地址",
          "accessMode": "r",
          "required": false,
          "dataType": "text",
          "dataSpecs": {"dataType": "text", "length": "256"}
        },
        {
          "identifier": "resolution",
          "name": "分辨率",
          "description": "视频分辨率",
          "accessMode": "r",
          "required": false,
          "dataType": "text",
          "dataSpecs": {"dataType": "text", "length": "32"}
        }
      ],
      "services": [
        {
          "identifier": "snapshot",
          "name": "抓拍快照",
          "description": "立即抓拍一张快照",
          "callType": "async",
          "required": false,
          "method": "thing.service.snapshot",
          "inputParams": [],
          "outputParams": [
            {
              "identifier": "snapshot_base64",
              "name": "快照数据",
              "direction": "output",
              "paraOrder": 0,
              "dataType": "text",
              "dataSpecs": {"dataType": "text", "length": "1000000"}
            }
          ]
        },
        {
          "identifier": "getPlayUrl",
          "name": "获取播放地址",
          "description": "获取实时播放URL",
          "callType": "async",
          "required": false,
          "method": "thing.service.getPlayUrl",
          "inputParams": [],
          "outputParams": [
            {
              "identifier": "play_url",
              "name": "播放地址",
              "direction": "output",
              "paraOrder": 0,
              "dataType": "text",
              "dataSpecs": {"dataType": "text", "length": "512"}
            }
          ]
        }
      ]
    },
    
